syntax = "v1"

type PostSingleReq {
	Message string `json:"message"`
}

type PostSingleRes {
	Message string `json:"message"`
}

service single-api {
	@doc "基础测试"
	@handler SingleHandler
	get /from/:name (PostSingleReq) returns (PostSingleReq)
}

service single-api {
	@doc "post测试"
	@handler PostDemoHandler
	post /post/single (PostSingleReq) returns (PostSingleRes)
}

